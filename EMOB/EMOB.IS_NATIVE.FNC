CREATE OR REPLACE FUNCTION EMOB.IS_NATIVE
RETURN
BOOLEAN IS
vNativeFlag VARCHAR2(1);
BEGIN
BEGIN
SELECT NVL(NATIVE_FLAG,'Y') INTO vNativeFlag FROM GUMS.MB_GLOBAL_RULE;
EXCEPTION
WHEN NO_DATA_FOUND
THEN
RAISE_APPLICATION_ERROR(-20001,'Native Or Non Native Mode Not Found.');
WHEN OTHERS
THEN
RAISE_APPLICATION_ERROR(-20001,'Native Or Non Native Mode Finding Problem.');
END;
IF vNativeFlag='Y'
THEN
RETURN TRUE;
ELSE
RETURN
FALSE;
END IF;
END;
/
